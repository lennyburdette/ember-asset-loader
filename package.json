{
  "name": "ember-asset-loader",
  "version": "0.4.1",
  "description": "Asset loading support for Ember applications",
  "keywords": [
    "ember-addon"
  ],
  "license": "MIT",
  "author": "Trent Willis <trentmwillis@gmail.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/ember-engines/ember-asset-loader",
  "scripts": {
    "build": "ember build",
    "lint:js": "eslint ./*.js addon addon-test-support app config lib server test-support tests",
    "start": "ember serve",
    "test": "ember test",
    "test:sauce": "ember sauce:connect && ember test --config-file testem.sauce.js --test-port 7000",
    "test:node": "mocha node-tests",
    "prepublish": "./bin/install-test-addons.sh"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.5",
    "broccoli-test-helper": "^1.1.0",
    "co": "^4.6.0",
    "ember-ajax": "^3.0.0",
    "ember-cli": "~2.18.2",
    "ember-cli-dependency-checker": "^2.0.1",
    "ember-cli-eslint": "^4.2.1",
    "ember-cli-htmlbars-inline-precompile": "^1.0.0",
    "ember-cli-htmlbars": "^2.0.1",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-qunit": "^4.1.1",
    "ember-cli-sauce": "1.6.0",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-resolver": "^4.0.0",
    "ember-source": "~2.18.0",
    "loader.js": "^4.2.3",
    "mocha": "^3.5.0",
    "test-generator-plugin": "file:./tests/dummy/lib/test-generator-plugin"
  },
  "dependencies": {
    "broccoli-caching-writer": "^3.0.3",
    "broccoli-funnel": "^1.0.8",
    "broccoli-merge-trees": "^2.0.0",
    "ember-cli-babel": "^6.7.2",
    "exists-sync": "^0.0.4",
    "fs-extra": "^4.0.1",
    "object-assign": "^4.1.0",
    "walk-sync": "^0.3.2"
  },
  "engines": {
    "node": "^4.5 || 6.* || >= 7.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
